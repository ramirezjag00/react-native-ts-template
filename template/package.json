{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn metro.kill && yarn start & react-native run-android",
    "ios": "yarn metro.kill && yarn start & react-native run-ios",
    "start": "react-native start",
    "pods": "npx pod-install",
    "reinstall": "rm -rf node_modules yarn.lock ios/Pods ios/Podfile.lock && yarn install && npx pod-install && yarn clear.cache",
    "clear.cache": "jest --clearCache && rm -rf $TMPDIR/metro-cache",
    "test": "jest",
    "lint": "eslint . --report-unused-disable-directives",
    "prettier.check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,md}\" \"App.tsx\"",
    "types": "tsc",
    "format": "prettier --loglevel warn --write \"src/**/*.{js,jsx,ts,tsx,md}\" \"App.tsx\" && eslint . --fix && tsc",
    "prepare": "husky",
    "bump-patch": "npm version patch --no-git-tag-version && fastlane bump_version",
    "bump-minor": "npm version minor --no-git-tag-version && fastlane bump_version",
    "bump-major": "npm version major --no-git-tag-version && fastlane bump_version",
    "metro.kill": "lsof -ti :8081 | xargs kill -9"
  },
  "lint-staged": {
    "\"src/**/*.{ts,tsx}\"|\"App.tsx\"": [
      "eslint --max-warnings=0",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-native": "0.78.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-react-jsx": "7.25.9",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.25.0",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.1",
    "@react-native/eslint-config": "0.78.1",
    "@react-native/metro-config": "0.78.1",
    "@react-native/typescript-config": "0.78.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@typescript-eslint/typescript-estree": "^7.8.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-compiler": "19.0.0-beta-aeaed83-20250323",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.19.0",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-barrel-files": "^1.2.1",
    "eslint-plugin-react-compiler": "19.0.0-beta-aeaed83-20250323",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^15.5.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  }
}
