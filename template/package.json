{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"App.tsx\"",
    "lintFix": "eslint \"src/**/*.{ts,tsx}\" \"App.tsx\" --fix",
    "prettier": "prettier --loglevel warn --write \"src/**/*.{ts,tsx,js}\"",
    "prepare": "husky install",
    "bump-patch": "npm version patch --no-git-tag-version && fastlane bump_version",
    "bump-minor": "npm version minor --no-git-tag-version && fastlane bump_version",
    "bump-major": "npm version major --no-git-tag-version && fastlane bump_version"
  },
  "lint-staged": {
    "\"src/**/*.{ts,tsx}\"|\"App.tsx\"": [
      "eslint --max-warnings=0",
      "prettier --write"
    ]
  },
  "engines": {
    "node": "^18",
    "npm": "^8 || ^9",
    "yarn": ">=1.22.17 <= 1.99.99"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.70.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@typescript-eslint/typescript-estree": "^5.7.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^12.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.4",
    "metro-react-native-babel-preset": "^0.73.3",
    "prettier": "^2.8.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
