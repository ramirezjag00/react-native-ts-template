{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn metro.kill && yarn start & react-native run-android",
    "ios": "yarn metro.kill && yarn start & react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "DEBUG=eslint:cli-engine eslint . --report-unused-disable-directives",
    "lint.fix": "eslint . --fix",
    "prettier": "prettier --loglevel warn --write \"src/**/*.{ts,tsx,js}\"",
    "types": "tsc",
    "prepare": "husky",
    "bump-patch": "npm version patch --no-git-tag-version && fastlane bump_version",
    "bump-minor": "npm version minor --no-git-tag-version && fastlane bump_version",
    "bump-major": "npm version major --no-git-tag-version && fastlane bump_version",
    "metro.kill": "lsof -ti :8081 | xargs kill -9"
  },
  "lint-staged": {
    "\"src/**/*.{ts,tsx}\"|\"App.tsx\"": [
      "eslint --max-warnings=0",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-native": "0.78.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-react-jsx": "7.25.9",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.25.0",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.1",
    "@react-native/eslint-config": "0.78.1",
    "@react-native/metro-config": "0.78.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@typescript-eslint/typescript-estree": "^7.8.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^15.5.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  }
}
